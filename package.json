    "nodemon": "^1.17.1",
    "semantic-release": "^15.0.2"
  },
  "dependencies": {
    "@settlemint/lib-crypto": "^4.7.0",
    "async": "^2.6.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "csv-parser": "^1.12.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "htmlparser": "^1.7.7",
    "mongodb": "^3.0.3",
    "node-schedule": "^1.3.0",
    "promise-retry": "^1.1.1",
    "request": "^2.83.0",
